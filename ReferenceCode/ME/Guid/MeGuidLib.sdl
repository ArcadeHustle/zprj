TOKEN
	Name  = "MeGuidLib_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable MeGuidLib support in Project"
	TokenType = Boolean
	TargetMAK = Yes
	Master = Yes
End

PATH
	Name  = "MeGuidLib_DIR"
End

MODULE
	Help  = "Includes MeGuidLib.mak to Project"
	File  = "MeGuidLib.mak"
End

TOKEN
	Name  = "MeGuidLib_LIB"
	Value  = "$$(LIB_BUILD_DIR)\MeGuidLib.lib"
	TokenType = Expression
	TargetMAK = Yes
End
