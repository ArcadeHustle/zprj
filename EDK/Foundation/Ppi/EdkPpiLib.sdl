TOKEN
	Name  = "EdkPpiLib_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable EdkPpiLib support in Project"
	TokenType = Boolean
	TargetEQU = Yes
	TargetMAK = Yes
	Master = Yes
End

TOKEN
	Name  = "EDKPPILIB"
	Value  = "$(BUILD_DIR)\EdkPpiLib.lib"
	TokenType = Expression
	TargetMAK = Yes
End

PATH
	Name  = "EdkPpiLib_DIR"
End

MODULE
	Help  = "Includes EdkPpiLib.mak to Project"
	File  = "EdkPpiLib.mak"
End

