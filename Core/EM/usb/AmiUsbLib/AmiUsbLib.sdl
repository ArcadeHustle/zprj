TOKEN
	Name  = "AmiUsbLib_SUPPORT"
	Value  = "1"
	Help  = "Main switch to enable AmiUsbLib support in Project"
	TokenType = Boolean
	TargetEQU = Yes
	TargetMAK = Yes
	TargetH = Yes
	Master = Yes
End

PATH
	Name  = "AMIUSBLIB_DIR"
End

MODULE
	Help  = "Includes AmiUsbSmmGlobalDataValidationLib.mak into project"
	Path  = "$(AMIUSBLIB_DIR)"
	File  = "AmiUsbSmmGlobalDataValidationLib.mak"
End
